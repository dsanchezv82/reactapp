{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "local-ios": {
      "ios": {
        "buildConfiguration": "Release"
      },
      "distribution": "internal",
      "developmentClient": false
    },
    "local-android": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "developmentClient": false
    }
  },
  "submit": {
    "production": {}
  }
}
